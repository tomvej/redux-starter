{
  "name": "redux-starter",
  "version": "1.0.0",
  "description": "Personal starter pack for redux applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomvej/redux-starter.git"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --mode development",
    "prebuild": "run-s lint test clean",
    "clean": "rimraf dist/*",
    "build": "webpack --progress --mode production",
    "lint": "eslint src tools",
    "lint:fix": "npm run lint -- --fix",
    "test:base": "mocha --require babel-register --require ignore-styles src/test.js \"src/**/*.spec.js\"",
    "test": "npm run test:base -- --reporter spec",
    "test:watch": "npm run test:base -- --reporter min --watch"
  },
  "author": "tom.vejpustek@centrum.cz",
  "license": "MIT",
  "dependencies": {
    "immutable": "3.8.2",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.3",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-loader": "7.1.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-enzyme": "1.0.0-beta.1",
    "chai-immutable": "1.6.0",
    "css-loader": "0.28.11",
    "dirty-chai": "2.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "html-webpack-plugin": "3.2.0",
    "ignore-styles": "5.0.1",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "react-test-renderer": "16.4.1",
    "redux-saga-test-plan": "3.7.0",
    "rimraf": "2.6.2",
    "sinon": "6.0.1",
    "sinon-chai": "3.2.0",
    "style-loader": "0.21.0",
    "webpack": "4.12.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  }
}
