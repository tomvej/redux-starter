{
  "name": "redux-starter",
  "version": "1.0.0",
  "description": "Personal starter pack for redux applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomvej/redux-starter.git"
  },
  "scripts": {
    "start": "webpack-dev-server -d --inline --hot --env.dev --progress",
    "prebuild": "npm run lint && npm run test && rimraf dist/*",
    "build": "webpack --progress --env",
    "lint": "eslint src tools",
    "lint:fix": "npm run lint -- --fix",
    "test:base": "mocha --require babel-register --require ignore-styles src/test.js \"src/**/*.spec.js\"",
    "test": "npm run test:base -- --reporter spec",
    "test:watch": "npm run test:base -- --reporter min --watch"
  },
  "author": "tom.vejpustek@centrum.cz",
  "license": "MIT",
  "dependencies": {
    "immutable": "3.8.2",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-immutable": "4.0.0",
    "redux-saga": "0.16.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-enzyme": "1.0.0-beta.0",
    "chai-immutable": "1.6.0",
    "css-loader": "0.28.7",
    "dirty-chai": "2.0.1",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "2.30.1",
    "ignore-styles": "5.0.1",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "mocha": "4.0.1",
    "react-test-renderer": "16.0.0",
    "redux-saga-test-plan": "3.3.1",
    "rimraf": "2.6.2",
    "sinon": "2.3.8",
    "sinon-chai": "2.14.0",
    "style-loader": "0.19.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  }
}
